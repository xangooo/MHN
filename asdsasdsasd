          ];
        let words = []; 
        let currentFocusIndex = -1;
        const gridEl = document.getElementById('word-grid');
        const meaningEl = document.getElementById('meaning-display');
        const fileInput = document.getElementById('file-upload');
        const toastEl = document.getElementById('toast');

        const countTotalEl = document.getElementById('count-total');
        const countKnownEl = document.getElementById('count-known');
        const countUnknownEl = document.getElementById('count-unknown');
        const countRemainingEl = document.getElementById('count-remaining');

        function init() {
            words = defaultWordData.map((item, index) => ({
                id: index, word: item.word, meaning: item.meaning, synonyms: item.synonyms || "", status: 'none'
            }));
            currentFocusIndex = 0;
            renderGrid();
            updateStats();
        }

        function renderGrid() {
            gridEl.innerHTML = '';
            words.forEach((item, index) => {
                const box = document.createElement('div');
                box.className = `word-box ${item.status}`;
                box.textContent = item.word;
                box.dataset.index = index;
                if (index === currentFocusIndex) box.classList.add('active-focus');
                box.addEventListener('click', () => { currentFocusIndex = index; renderGrid(); });
                gridEl.appendChild(box);
            });
        }

        function reshuffleWords() {
            for (let i = words.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [words[i], words[j]] = [words[j], words[i]];
            }
            currentFocusIndex = 0;
            renderGrid();
            clearMeaning();
            showToast("List reshuffled.");
        }

        function sortUnknownFirst() {
            const sortedWords = [...words].map((w, i) => ({ ...w, originalIndex: i }));
            sortedWords.sort((a, b) => {
                const getStatusWeight = (status) => {
                    if (status === 'unknown') return 0;
                    if (status === 'none') return 1;
                    if (status === 'known') return 2;
                    return 1;
                };
                const weightA = getStatusWeight(a.status);
                const weightB = getStatusWeight(b.status);
                if (weightA !== weightB) return weightA - weightB;
                return a.originalIndex - b.originalIndex;
            });
            words = sortedWords.map((w, i) => ({ ...w, id: i }));
            currentFocusIndex = 0;
            renderGrid();
            showToast("Sorted: Unknown first.");
        }

        function updateStats() {
            const total = words.length;
            const known = words.filter(w => w.status === 'known').length;
            const unknown = words.filter(w => w.status === 'unknown').length;
            const remaining = total - known - unknown;
            countTotalEl.textContent = total;
            countKnownEl.textContent = known;
            countUnknownEl.textContent = unknown;
            countRemainingEl.textContent = remaining;
        }

        function updateWordStatus(status) {
            if (currentFocusIndex === -1 || currentFocusIndex >= words.length) return;
            words[currentFocusIndex].status = status;
            const boxes = document.querySelectorAll('.word-box');
            const box = boxes[currentFocusIndex];
            box.classList.remove('known', 'unknown');
            if (status !== 'none') box.classList.add(status);
            updateStats();
        }

        function toggleMeaning() {
            if (currentFocusIndex === -1 || currentFocusIndex >= words.length) return;
            const currentWord = words[currentFocusIndex];
            if (meaningEl.dataset.currentId === String(currentWord.id) && meaningEl.innerHTML !== "") {
                clearMeaning();
            } else {
                meaningEl.dataset.currentId = currentWord.id;
                meaningEl.innerHTML = `<strong>${currentWord.word}</strong><br><br>${currentWord.meaning}`;
            }
        }

        function clearMeaning() { meaningEl.textContent = ""; meaningEl.removeAttribute('data-current-id'); }
        function resetToDefault() { if(confirm("Reset to default list?")) { init(); showToast("Reset."); } }
        function showToast(msg) { toastEl.textContent = msg; toastEl.classList.add('show'); setTimeout(() => toastEl.classList.remove('show'), 3000); }

        // ==========================================
        // ADVANCED PARSING (PDF / WORD / EXCEL)
        // ==========================================
        fileInput.addEventListener('change', async (e) => {
            const file = e.target.files[0];
            if (!file) return;
            showToast("Processing file...");
            
            try {
                let newWords = [];
                if (file.name.match(/\.(xlsx|xls|csv)$/)) {
                    newWords = await parseExcel(file);
                } else if (file.name.match(/\.(doc|docx)$/)) {
                    newWords = await parseWord(file);
                } else if (file.name.match(/\.pdf$/)) {
                    newWords = await parsePDF(file);
                } else {
                    showToast("Unsupported format.");
                    return;
                }

                if (newWords.length > 0) {
                    loadNewData(newWords);
                    showToast(`Loaded ${newWords.length} words.`);
                } else {
                    showToast("No valid words found.");
                }
            } catch (err) {
                console.error(err);
                showToast("Error reading file.");
            }
            fileInput.value = '';
        });

        function parseExcel(file) {
            return new Promise((resolve, reject) => {
                const reader = new FileReader();
                reader.onload = (e) => {
                    const data = new Uint8Array(e.target.result);
                    const workbook = XLSX.read(data, { type: 'array' });
                    const firstSheet = workbook.Sheets[workbook.SheetNames[0]];
                    const jsonData = XLSX.utils.sheet_to_json(firstSheet, { header: 1 });
                    const parsed = [];
                    let startIndex = 0;
                    if (jsonData.length > 0 && typeof jsonData[0][1] === 'string' && jsonData[0][1].toLowerCase().includes('meaning')) startIndex = 1;
                    for (let i = startIndex; i < jsonData.length; i++) {
                        const row = jsonData[i];
                        if (row && row.length > 0) {
                            parsed.push({ word: row[0] || "Unknown", meaning: row[1] || "", synonyms: row[2] || "" });
                        }
                    }
                    resolve(parsed);
                };
                reader.readAsArrayBuffer(file);
            });
        }

        function parseWord(file) {
            return new Promise((resolve, reject) => {
                const reader = new FileReader();
                reader.onload = (e) => {
                    mammoth.extractRawText({ arrayBuffer: e.target.result })
                        .then(result => resolve(parseTextLines(result.value)))
                        .catch(err => reject(err));
                };
                reader.readAsArrayBuffer(file);
            });
        }

        async function parsePDF(file) {
            const typedarray = await file.arrayBuffer();
            const pdf = await pdfjsLib.getDocument(typedarray).promise;
            let fullText = "";
            
            for (let i = 1; i <= pdf.numPages; i++) {
                const page = await pdf.getPage(i);
                const textContent = await page.getTextContent();
                
                // Heuristic: Identify the boundary between Left Column (Words) and Right Column (Meanings)
                // We find the x-coordinate where the right column starts.
                let items = textContent.items;
                if (items.length === 0) continue;

                // 1. Group text by Y-coordinates (roughly line by line)
                // PDF coordinates: x (horizontal), y (vertical, inverted)
                const lineMap = {};
                items.forEach(item => {
                    // Round Y to handle slight variations
                    const y = Math.round(item.transform[5]); 
                    if (!lineMap[y]) lineMap[y] = [];
                    lineMap[y].push(item);
                });

                // 2. Sort Y coordinates from top to bottom
                const sortedY = Object.keys(lineMap).sort((a, b) => b - a);

                // 3. Determine column split point (X-axis)
                // We assume the first line gives a hint: Word is left, Meaning is right.
                const firstLineItems = lineMap[sortedY[0]];
                // Split roughly in the middle of the width of the page, or use distinct gaps
                // Let's use the first gap between items in the first line
                let splitX = 300; // Default fallback
                if (firstLineItems.length >= 2) {
                    // Sort first line items by X position
                    firstLineItems.sort((a, b) => a.transform[4] - b.transform[4]);
                    // Assume gap between word 1 and word 2 is the column split
                    splitX = (firstLineItems[0].transform[4] + firstLineItems[firstLineItems.length - 1].transform[4]) / 2;
                }

                // 4. Process each line
                sortedY.forEach(y => {
                    const lineItems = lineMap[y];
                    lineItems.sort((a, b) => a.transform[4] - b.transform[4]); // Sort left to right

                    let wordPart = "";
                    let meaningPart = "";

                    // Heuristic: If items are far apart, it's 2 columns. If close, it's 1 long line.
                    for (let i = 0; i < lineItems.length; i++) {
                        const item = lineItems[i];
                        const nextItem = lineItems[i+1];

                        // Check gap to next item
                        const hasGap = nextItem && (nextItem.transform[4] - (item.transform[4] + item.width)) > 30;

                        if (i === 0) {
                            wordPart = item.str;
                        } else if (item.transform[4] > splitX && i > 0) {
                            // If we are past the split point, this belongs to Meaning
                            meaningPart += item.str + " ";
                        } else if (hasGap) {
                            // If we are still left of split but there is a huge gap (e.g. number list), treat as start of meaning
                            meaningPart += item.str + " ";
                        } else {
                            // Continuation of word
                            wordPart += item.str + " ";
                        }
                    }

                    if (wordPart && meaningPart) {
                         // Add temporary object to fullText string for processing later
                         // We use a unique separator that won't appear in text
                         fullText += `${wordPart.trim()} ||| ${meaningPart.trim()}\n`;
                    } else if (wordPart) {
                        // Single column case or parsing error, add to meaning as fallback
                         fullText += `${wordPart.trim()} ||| \n`;
                    }
                });
            }
            
            // Now parse our constructed text
            return parseTextLines(fullText, " ||| ");
        }

        function parseTextLines(text, separator = /\s-\s|\s\|\s|\t/) {
            const lines = text.split(/\r?\n/);
            const parsed = [];
            lines.forEach(line => {
                line = line.trim();
                if(!line) return;
                // Split by separator or our special PDF separator
                const parts = line.split(separator);
                
                // If we have 2 parts (Word | Meaning), use them. 
                // If we have more, maybe Word | Meaning | Synonym.
                if (parts.length >= 2) {
                    parsed.push({
                        word: parts[0].trim(),
                        meaning: parts[1].trim(),
                        synonyms: parts[2] ? parts[2].trim() : ""
                    });
                }
            });
            return parsed;
        }

        function loadNewData(newItems) {
            words = newItems.map((item, index) => ({
                id: index, word: item.word, meaning: item.meaning, synonyms: item.synonyms, status: 'none'
            }));
            currentFocusIndex = 0;
            renderGrid();
            updateStats();
            clearMeaning();
        }

        // Keyboard
        document.addEventListener('keydown', (e) => {
            if (['INPUT', 'TEXTAREA'].includes(document.activeElement.tagName)) return;
            const key = e.key.toLowerCase();
            if (key === 'y') updateWordStatus('known');
            else if (key === 'n') updateWordStatus('unknown');
            else if (key === ' ') { e.preventDefault(); toggleMeaning(); }
            else if (['arrowdown', 'arrowright'].includes(key)) { e.preventDefault(); moveFocus(1); }
            else if (['arrowup', 'arrowleft'].includes(key)) { e.preventDefault(); moveFocus(-1); }
        });

        function moveFocus(direction) {
            if (words.length === 0) return;
            let newIndex = currentFocusIndex + direction;
            if (newIndex < 0) newIndex = 0;
            if (newIndex >= words.length) newIndex = words.length - 1;
            currentFocusIndex = newIndex;
            const boxes = document.querySelectorAll('.word-box');
            boxes.forEach(b => b.classList.remove('active-focus'));
            if (boxes[currentFocusIndex]) {
                boxes[currentFocusIndex].classList.add('active-focus');
                boxes[currentFocusIndex].scrollIntoView({ behavior: 'smooth', block: 'nearest' });
            }
        }

        init();
    </script>
</body>
</html>
